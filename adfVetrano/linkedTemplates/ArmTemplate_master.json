{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfVetrano"
		},
		"AzureBlobStorageVetrano_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageVetrano'"
		},
		"AzureSqlDatabaseVetrano_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseVetrano'"
		},
		"RestServiceVetrano_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.polygon.io/"
		},
		"kvvetranomastercloud_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvvetranomastercloud.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfVetrano_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageVetrano_connectionString": {
						"value": "[parameters('AzureBlobStorageVetrano_connectionString')]"
					},
					"AzureSqlDatabaseVetrano_connectionString": {
						"value": "[parameters('AzureSqlDatabaseVetrano_connectionString')]"
					},
					"RestServiceVetrano_properties_typeProperties_url": {
						"value": "[parameters('RestServiceVetrano_properties_typeProperties_url')]"
					},
					"kvvetranomastercloud_properties_typeProperties_baseUrl": {
						"value": "[parameters('kvvetranomastercloud_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}